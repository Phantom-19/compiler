# -Chargement a ma deuxieme methode-#
def load(word):
    mr = [
     '/', '-', '|']
    for i in range(5):
        for x in range(len(mr)):
            sys.stdout.write(('\r{}{}').format(str(word), mr[x]))
            time.sleep(0.3)
            sys.stdout.flush()
