def binnar():
    script = raw_input('File >> ')           
    bc = open(script).read() 
    js = raw_input('Resultat >> ' )      
    gx = base64.b32encode(bc)         
    a = []           
    b = []          
    c = []          
    d = ''            
    e = ''            
    for x in gx:               
        a.append(ord(x))           
        z = 0            
        while True:               
           if z >= len(a) / 2:            
               break             
               z += 1            
               v = len(a) / 2 + 1          
        try:                
            while True:                  
              v += 1           
        except IndexError:           
             pass          
        for s in c:            
             d += chr(s)            
             e += bin(int(binascii.hexlify(d), 16))            
             f = open(js, 'w')            
             f.write('#Compile Faxel\n#Twitter https://twitter.com/Faxel2020\nimport binascii,base64\ng = ' + str(b) + "\ns = '" + str(e) + "'" + "\neval(compile(base64.b32decode(''.join([chr(i) for i in g])+binascii.unhexlify('%x' % int(s, 2))),'<sazxt>','exec'))") 
             f.close()   
binnar()

def zlib_s():
       script = raw_input('File >> ')     
       resultat = raw_input('Resultat >> ' ) 
       lecture = open(script).read()                
       fin = zlib.compress(lecture)         
       s = open(resultat, 'w')               
       s.write('#Compile Faxel\n#Twitter https://twitter.com/Faxel2020\nimport zlib\nexec(zlib.decompress(' + repr(fin) + '))')                
       s.close()
zlib_s()

