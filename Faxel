# -Place au choix
def select():    
  choisir = raw_input("\033[1;97m‚ïö‚ïê\033[1;31m‚ñ∂[38;5;245m Mr [38;5;221mFaxel \033[1;31m‚ñ∂‚ñ∂‚ñ∂ \033[1;33m")
  if choisir == "1" or choisir == "01":
       marshal()
       raw_input("\n\033[1;91m[\033[38;5;245m Retour\033[1;91m]")
       menu()
  elif choisir == "2" or choisir == "02":
       zlib_mars()  
       raw_input("\n\033[1;91m[\033[38;5;245m Retour\033[1;91m]")
       menu()
  elif choisir == "3" or choisir == "03":
       mars()  
       raw_input("\n\033[1;91m[\033[38;5;245m Retour\033[1;91m]")
       menu()
  elif choisir == "4" or choisir == "04":
       binnar()   
       raw_input("\n\033[1;91m[\033[38;5;245m Retour\033[1;91m]")
       menu()
  elif choisir == "5" or choisir == "05":
       zlib_s()   
       raw_input("\n\033[1;91m[\033[38;5;245m Retour\033[1;91m]")
       menu()
  elif choisir == "6" or choisir == "06":
       zlib_lod()   
       raw_input("\n\033[1;91m[\033[38;5;245m Retour\033[1;91m]")
       menu()   
  elif choisir == "q" or choisir == "Q":
       exit()
  else: 
       print ('\x1b[1;91m[\xe2\x9c\x96] \x1b[1;97m' + choisir + ' \x1b[1;91mindisponible')
       raw_input('\033[1;91m [\033[1;97m!\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu\033[1;93m...')
       menu()
    
def marshal():
      script = raw_input('File >> ')    
      lecture = open(script).read()  
      resultat = raw_input('Resultat >> ') 
      compresse = compile(lecture, '<Faxel>', 'exec')        
      debut = marshal.dumps(compresse)             
      fin = repr(debut)             
      s = open(resultat, 'w')             
      s.write('#Compile Faxel\n#Twitter https://twitter.com/Faxel2020\nimport marshal\nexec(marshal.loads(' + str(fin) + '))')         
      s.close() 
 
def zlib_mars():
    script = raw_input('File >> ')            
    a = open(script).read()    
    js = raw_input('Resultat >> ' )      
    z = []            
    u = []  
    o = []  
    for x in a:       
          z.append(ord(x))   
    for c in z:         
          u.append(zlib.compress(str(c)))               
    for p in u:         
          o.append(marshal.dumps(p))         
          fip = '#Compile Faxel\n#Twitter https://twitter.com/Faxel2020\nimport marshal\nimport zlib\nd={}\nexec("".join([chr(int(zlib.decompress(marshal.loads(i)))) for i in d]))'.format(o)            
          ox = open(js, 'w')            
          ox.write(fip)   
          ox.close()          
               
def mars():
    script = raw_input('File >> ' )            
    bc = open(script).read()            
    js = raw_input('Resultat >> ' )         
    a = []          
    b = []           
    bin = ''    
    xz = []  
    xh = compile(bc, '<Faxel>', 'exec')            
    pg = marshal.dumps(xh) 
    bin += binascii.hexlify(pg)          
    for g in bin:               
          a.append(ord(g) - 9)           
    for lx in range(len(a)):  
          b.append(randint(1, 120))          
          c = []          
          d = 0            
    for x in b:               
          c.append(9 * b[d])           
          d += 1    
          x_x = lambda : randint(1, 255)                     
    for asx in range(255):             
          xz.append(x_x())          
          na = open(js, 'w')            
          na.write('z = ' + str(xz) + '\n')           
          na.write('_ = ' + str(b) + '\n__ = ' + str(c) + '\nOoO_ = ' + str(a) + '\n#Compile Faxel\n#Twitter https://twitter.com/Faxel2020\n\nimport marshal\nOO = lambda _ : marshal.loads(_)\nu = ( ( { } < ( ) ) - ( { } < ( ) ) )\np = (({}<())-({}<()))\nv = []\nexec((lambda:((()>())+(()<()))).func_code.co_lnotab).join(map(chr,[(((((((({}=={})+([]>={})+({}<[]))+(({}<[])+({}<[])+({}!=[]))+((()>{})*([]<=())))<<((({}>={})+(()>{}))))-((({}<=[])+({}>{})))))<<((({}<[])+(()==()))))),((((((((()==())*(()<=())))<<((([]>=[])+({}=={}))+((()==())+(()>={}))))-((([]>={})*({}!=[])))))<<((({}=={})+({}<[])+(()!={}))))),((((([]==())+([]!={})))<<((({}<[])+({}<[]))+((()>=[])+({}<()))+(({}!=[])+(()<=[]))))),(((((((({}>=())+({}!=())))<<(((()>{})+([]<=()))+(({}!=())+({}<=()))))-((([]==())+({}<={})))))<<((([]>={})+({}<=[]))))+((([]==[])+({}==())))),((((({}>={})-(()!=())))<<(((()>=())+({}<=[]))+((()>{})+([]>{}))+((()==())-([]<{}))))),(((((((([]==[])+(()>{})+(()>{})))<<(((()>[])+(()>={})+([]>=[]))))-(((()!={})+({}<{})))))<<((({}!=[])+({}>={}))))-((({}<())*([]==[])))),((((((((()==())+({}!=[])+({}<[])))<<(((()<=())+({}<[])+({}>={}))))-((({}<=[])+({}!={})))))<<((([]!={})+({}=={}))))+((([]<())-({}>[])))),((((({}>={})+([]<()))+((()!=[])+(()!={}))+(({}!=[])*({}<())))<<(((()>={})+([]>()))))),(((((((((({}<=())+({}!=())+({}<[])))<<((({}=={})+([]!={}))))+(((()<=())-([]<{})))))))<<((({}<())+({}<[])+(()>={}))))-((((()!=())+(()>[])))<<(((()<[])+(()>={}))))),(((((()!=[])+(()>={})+(()>=[]))+(({}<[])+([]<=())+(()==()))+(({}==())+(()!=[])))<<((({}!=())+([]!=()))+(({}<())+({}>={}))))-((({}<={})-(()<{})))),((((([]>={})+([]!={})+([]>={}))+((()<=())+([]>={})+(()!=[]))+((()!={})+([]<={})))<<((([]!=())+({}<[]))+(({}<())+({}!=[]))))+(((({}>={})+([]<[])))<<((({}<[])+(()<={}))))),(((((()>=())+(()<())))<<((([]!=())+({}<={}))+((()>=[])+({}<[]))+((()!=[])+(()!=()))))),((((({}<[])+([]==[])+([]>{})))<<((([]>{})+([]!={}))+((()>=())+({}<=[]))+(([]<{})+({}=={}))))-(((()>=[])*(()!={})))),((((((((()>{})+(()!=())))<<((([]>=[])+((