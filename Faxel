def dec_base64():
   script = raw_input(" Entrer le script a decrypter > ")
   resultat = raw_input(" Nommer le resultat > ")
   print("Attente du decryptage ...")
   print("-"*40)
   try:
   	ouverture = open(script).readlines()
   except:
   	print("N'a pas r√©ussi √† ouvrir la liste")
   	exit()
   sauve=[]
   jabot=[]
   sauvegarde=open(resultat,'w')
   for k in ouverture:
   	try:
   		print("Resultat -> ",base64.b64decode(k.split('\n')[0]))
   		resul = base64.b64decode(k.split('\n')[0])
   		jabot.append(resul)
   		time.sleep(00.01)
   	except:
   		print ("Desol√© se script n'a pas √©t√© crypter en base64")
   		sauve.append(k)
   		time.sleep(00.01)
   print("-"*40)
   for result in jabot:
   	sauvegarde.write(result.split('\n')[0]+"\n")
   sauvegarde.close()

