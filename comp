def menu_ip():	
	cls()
	print(logo_menu_ip)
	horaire()
	print("[38;5;214mğŸ‡¨ğŸ‡®\033[1;97m I'm the person whom you can never judge. \033[1;92m ğŸ‡¨ğŸ‡®")
