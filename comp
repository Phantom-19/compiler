	print("\033[1;97mâ•”"+61*"â•"+1*"â•â•—")
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96m1.\033[1;91m][48;5;0;38;5;197m HÃ´te d\'ecoulement de masse vers IP avec Web liste.\033[1;97m      â•‘")
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96m2.\033[1;91m][48;5;0;38;5;158m IP unique\033[1;91m/[48;5;0;38;5;158mnom d\'hÃ´te.\033[1;97m                                   â•‘")
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96m3.\033[1;91m][38;5;114m VÃ©rifier mon adresse IP publique.\033[1;97m                       â•‘")
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96m4.\033[1;91m][38;5;74m Generer des portes derobÃ©es Py pour obtenir l\'adresse \033[1;97m, â•‘")
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96mâ†ªï¸.\033[1;91m][38;5;74m IP de la victime publique.\033[1;97m                              â•‘") 
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96m5.\033[1;91m][38;5;210m Emplacement de vidage Ã  partir de l\'adresse IP publique.\033[1;97mâ•‘")
	print("\033[1;97mâ•‘ \033[1;91m[\033[1;96m6.\033[1;91m][48;5;0;38;5;198m Liens de page d\'extraction en masse Ã  partir de l\'hÃ´te.\033[1;97m â•‘")
